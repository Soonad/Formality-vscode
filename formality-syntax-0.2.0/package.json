{
	"name": "fm",
	"displayName": "Formality",
	"description": "Syntax highlighting for Formality",
	"version": "0.2.0",
	"publisher": "SunshineCybernetics",
	"author": {
		"name": "Maisa Milena"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"keywords": [
		"formality",
		"fm"
	],
	"icon": "images/logo-formality.png",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "fm",
				"aliases": [
					"Formality",
					"fm"
				],
				"extensions": [
					".fm"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "fm",
				"scopeName": "source.fm",
				"path": "./syntaxes/fm.tmLanguage.plist"
			}
		],
		"snippets": [
      {
        "language": "fm",
        "path": "./snippets/fm.json"
      }
    ]
	},
	"bugs": {
		"url": "https://github.com/MaisaMilena/Formality-vscode/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MaisaMilena/Formality-vscode.git"
	},
